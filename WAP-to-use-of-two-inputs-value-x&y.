#include<stdio.h>
int main () {
    int x,y;
    printf("Enter the value of x:");
    scanf("%d",&x);
    printf("Enter the value of y:");
    scanf("%d",&y);
    x>=y;
    x<=y;
    x==y;
    x!=y;
    x>y;
    x<y;
    printf("result of \n>=%d \n<=%d \n==%d \n!=%d \n>%d \n<%d",x>=y, x<=y, x==y, x!=y, x>y,
    x<y);
    return 0;
}
